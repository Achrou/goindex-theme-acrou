    "vue-clipboard2": "^0.3.1"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "html-webpack-plugin": "^4.2.0",
    "vue": "^2.6.11",
    "vue2-ace-editor": "0.0.15",
    "webpack": "^4.42.1",
